{
  "expo": {
    "name": "Guahan Grocer",
    "slug": "guahan-grocer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "guahangrocer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Fresh groceries delivered to your door on Guam",
    "privacy": "public",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F766E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.guahangrocer.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to calculate accurate delivery distances and fees.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos of receipts during checkout."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0F766E"
      },
      "package": "com.guahangrocer.app",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "12feee7a-8a4a-4883-8801-5e3bd7287bed"
      }
    },
    "owner": "shimizutechnology"
  }
}
